{% extends 'single.jinja' %}

{% block container %}
  <ul class="list-group">
    {% for entry in entries %}
      <a href="{{ url_for('product.attr.manifest', id=entry.id) }}" class="list-group-item">{{ entry.r_product.name }} - {{ entry.name }}</a></li>
    {% endfor %}
  </ul>
{% endblock container %}
