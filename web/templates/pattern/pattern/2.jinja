{% extends 'pattern/index.jinja' %}

{% block container %}
  <h3>버전 선택</h3>
  <ul class="list-group">
    {% for entry in entries %}
      <a class="list-group-item" href="{{ url_for('pattern.version.view', product=entry.product, pattern=entry.pattern) }}">{{ '{} - {}.{:02}'.format(entry.pattern, entry.r_version.date, entry.r_version.extend) }}</a>
    {% endfor %}
  </ul>
  <div>{{ pagination(pageData) }}</div>
{% endblock container %}
